{
  "name": "java-run",
  "displayName": "Java Run",
  "description": "一键编译运行Java代码",
  "version": "1.1.2",
  "publisher": "caolin",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "bugs": {
    "url": "https://github.com/NeverCL/vscode-java-run/issues"
  },
  "homepage": "https://github.com/NeverCL/vscode-java-run",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeverCL/vscode-java-run.git"
  },
  "activationEvents": [
    "onCommand:java.run"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "java.run",
        "title": "Java.Run"
      }
    ],
    "keybindings": [
      {
        "command": "java.run",
        "key": "alt+b",
        "mac": "alt+b",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "iconv-lite": "^0.4.15"
  }
}